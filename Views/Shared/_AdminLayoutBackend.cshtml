<script type="text/javascript">
    $('#bookingExtraSelectionSort').on('change', function () {
        ResizeToStandard();
        //delete existing
        $('#TopEditorWindow').empty();
        //get all vars
        var BookingExtraSelectionId = $("#bookingExtraSelectionSort").val();

        //request
        $.ajax({
            url: '@Url.Action("Edit", "BookingExtraSelection")',
            data: { "BookingExtraSelectionId": BookingExtraSelectionId },
            type: "GET",
            success: function (result) {
                $('#TopEditorWindow').html(result);
                //get the pricing
                getBookingExtraSelectionParticipants(BookingExtraSelectionId);
            },
            error: function () { alert('That did not work please try again'); }
        });

    });


    function getBookingExtraSelectionParticipants(BookingExtraSelectionId) {
        $.ajax({
            url: '@Url.Action("Index", "BookingExtraParticipant")',
            data: { "BookingExtraSelectionId": BookingExtraSelectionId },
            type: "GET",
            success: function (result) { $('#BottomEditorWindow').html(result); },
            error: function () { alert('That did not work second call'); }
        });
    }


</script>





@*Property Town*@
<script type="text/javascript">
    $('#propertyTownSort').on('change', function () {
        ResizeToStandard();
        //delete existing
        $('#TopEditorWindow').empty();
        $('#BottomEditorWindow').empty();

        //get all vars
        var id = $("#propertyTownSort").val();

        //request
        $.ajax({
            url: '@Url.Action("Edit", "PropertyTown")',
            data: { "id": id },
            type: "GET",
            success: function (result) {
                $('#BottomEditorWindow').animate({ height: '60%' }, 500);
                $('#TopEditorWindow').animate({ height: '40%' }, 500);
                $('#TopEditorWindow').html(result);
                //get the pricing
            },
            error: function () { alert('That did not work please try again'); }
        });

    });

</script>

