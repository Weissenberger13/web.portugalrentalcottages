@* Added by ND - show / hide the spinner on ajax call*@
@*hide spinner*@
<script type="text/javascript">
    $(document)
        .ajaxStart(function () {
            $("#spinner").show();
        })
        .ajaxStop(function () {
            $("#spinner").hide();
        });

    $(window).load(function () {
        $("#spinner").fadeOut(1000);
    });

    $("a", "input:submit", "button").click(function () {
        $("#spinner").show();
    });


</script>


@*Property*@
<script type="text/javascript">
    $('#propertySort').on('change', function () {

        $('#BottomEditorWindow').show();
        $('#BottomEditorWindow').animate({ height: '60%' }, 500);
        $('#TopEditorWindow').animate({ height: '40%' }, 500);
        //delete existing
        $('#TopEditorWindow').empty();
        //get all vars
        var propID = $("#propertySort").val();

        $("#spinner").show();

        //request
        $.ajax({
            url: '@Url.Action("SinglePropertyIndex", "Property")',
            data: { "PropertyID": propID },
            type: "GET",
            success: function (result) {
                $('#TopEditorWindow').html(result);
                //get the pricing
                getPropertyPricings(propID);
            },
            error: function () { alert('That did not work please try again'); }
        });

    });

    function getPropertyPricings(propID) {
        $.ajax({
            url: '@Url.Action("Index", "PropertyPricingSeasonalInstance")',
            data: { "PropertyID": propID },
            type: "GET",
            success: function (result) { $('#BottomEditorWindow').html(result); },
            error: function () { alert('That did not work please try again'); }
        });
    }


</script>


