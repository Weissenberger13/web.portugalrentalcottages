@using BootstrapVillas.Models
@using BootstrapVillas.Content.Classes;
@using WebGrease.Css.Extensions



@{


    AjaxOptions options = new AjaxOptions
    {
        UpdateTargetId = "TopEditorWindow",
        InsertionMode = InsertionMode.Replace,
        LoadingElementId = "spinner",


    };




    var resultAmountForDDL = new Dictionary<string, string>();
    resultAmountForDDL.Add("0", "Show all");
    resultAmountForDDL.Add("10", "10");
    resultAmountForDDL.Add("20", "20");
    resultAmountForDDL.Add("50", "50");
    resultAmountForDDL.Add("100", "100");

    List<Property> props = new List<Property>();
    List<BookingExtra> bookingExtras = new List<BookingExtra>();

    var propertyTown = new List<PropertyTown>();
    var propertyRegion = new List<PropertyRegion>();
    var propertyOwner = new List<PropertyOwner>();
    var propertyOwnerRep = new List<PropertyOwnerRepresentative>();

    //instances
    var booking = new List<Booking>();
    var bookingExtraSelections = new List<BookingExtraSelection>();
    var custs = new List<Customer>();





    using (var _db = new PortugalVillasContext())
    {
        props = Property.GetAllActiveProperties().OrderBy(x => x.LegacyReference).ToList();

        bookingExtras = BookingExtra.GetBookingExtras();

        propertyTown = _db.PropertyTowns.OrderBy(x => x.TownName).ToList();
        propertyRegion = _db.PropertyRegions.ToList(); //town and region together

        propertyOwner = _db.PropertyOwners.OrderBy(x => x.OwnerLastName).ToList();
        propertyOwnerRep = _db.PropertyOwnerRepresentatives.ToList();

        //instances that are NOT 'deleted'
        booking = _db.Bookings.Where(x => x.Cancelled.Equals(false)).ToList(); //bookings, booking participants
        bookingExtraSelections = _db.BookingExtraSelections.Where(x => x.Cancelled.Equals(false)).ToList(); //selections, selection participants
        custs = _db.Customers.ToList(); //customers, logons*/
    }


    //generate DDLS


}

