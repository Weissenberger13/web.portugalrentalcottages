    //currencyDefaults
    var defaultCurrency = (string)HttpRuntime.Cache["defaultCurrency"];
    var currencySymbol = (string)HttpRuntime.Cache["currencySymbol"];
    var defaultCurrencyEnum = (CurrencyEnum) Enum.Parse(typeof (CurrencyEnum), (string)HttpRuntime.Cache["defaultCurrency"]);



    //KEYWORDS
            if (ViewBag.Keywords == null || ViewBag.Keywords == "")
    {
        ViewBag.Keywords = "holiday portugal, portugal rental cottages, portugal holiday rental, holiday villa portugal,silver coast villas,portugal cheap holidays";
    }


    var ajaxOptions = new AjaxOptions
    {
        InsertionMode = InsertionMode.Replace
    };

    var ruralCollection = new FormCollection();
    ruralCollection.Add("PropertyType", "1");


    <!-- View Methods -->


    DirectoryInfo Server = (DirectoryInfo)ViewBag.theServerPath;

    @*Get the current appID for the bing translator*@
    string AppID = System.Web.HttpContext.Current.Application["TranslateToken"].ToString();

    List<SelectListItem> langs = new List<SelectListItem>();
    langs.Add(new SelectListItem { Text = "English", Value = "en", Selected = true });
    langs.Add(new SelectListItem { Text = "Français", Value = "fr" });
    langs.Add(new SelectListItem { Text = "Deutsche", Value = "de" });
    langs.Add(new SelectListItem { Text = "Español", Value = "es" });
    langs.Add(new SelectListItem { Text = "Portugués", Value = "pt" });
    langs.Add(new SelectListItem { Text = "Ruso", Value = "ru" });

    BookingExtraSelection currentBookingExtraSelection = (BookingExtraSelection)Session["CurrentBookingExtraDataGathering"];

    List<BookingExtra> CarBookingExtras = BookingExtra.GetBookingExtras();
    var phrInfo = PRCInformation.GetPRCInformation();


    //test if logged in, make sure there's something in session if so:

    if (!WebSecurity.CurrentUserName.Equals("") && WebSecurity.CurrentUserName != null)
    {
        try
        {
            if (Session["prc_customer"] == null)
            {
                var customer = Customer.GetCustomerByEmailAddress(WebSecurity.CurrentUserName);
                Session["prc_customer"] = customer;

                if (Session["prc_customerBankingDetail"] == null)
                {

                    Session["prc_customerBankingDetail"] = CustomerBankDetail.GetCustomerBankDetailFromCustomer(customer);
                }
            }


        }
        catch (Exception)
        {


        }

    }

    //set defaults for aiport and carhire if there is a booking in the session
    //ONLY THE FIRST BOOKING
    //PASS IN THROUGH VIEWDATA TO partial
    Booking firstBooking = null;
    if (Session["Cart_PropertyBookings"] != null)
    {

        List<Booking> bookings = (List<Booking>)Session["Cart_PropertyBookings"];

        if (bookings.Count > 0)
        { firstBooking = bookings.First(); }

    }

    var extraServiceCharges = PropertyTypeServicesChargeInstance.GetExtraTypeServicesChargeInstances();

}

